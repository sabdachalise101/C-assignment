/*Wap Calculate simple interest for given 𝑃=4000,𝑇=2,𝑅=5.5P=4000,T=2,R=5.5*/

#include <stdio.h>
#include <conio.h>

int main() {
    int num1, num2, num3;
    printf("Enter any 3 integers: ");
    scanf("%d %d %d", &num1, &num2, &num3);
    printf("\nThe integers in forward order: %d %d %d", num1,num2,num3);
    printf("\nThe integers in reversed order: %d %d %d", num3,num2,num1);
    printf("\nSabda Chalise");
    getch();
    return 0;
}
