#include <stdio.h>
#include <conio.h>

int main() {
    int num1,num2,sum;
    printf("Enter the first number: ");
    scanf("%d",&num1);
    printf("Enter the second number: ");
    scanf("%d",&num2);
    sum=num1+num2;
    printf("\nThe sum is: %d.",sum);
    printf("\nSabda Chalise");
    getch();
    return 0;
}
